<?xml version='1.0' encoding='utf-8'?>
<widget id="com.pdp.BdayReminder" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>bdayReminder</name>
    <description>A app to help you remember your friends' birthday by sending you notifications.</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-navigation href="about:*" />
    <preference name="loadUrlTimeoutValue" value="700000" />
</widget>
